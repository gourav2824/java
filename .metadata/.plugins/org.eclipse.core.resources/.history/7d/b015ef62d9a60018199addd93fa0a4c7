import java.util.Scanner;

public class Pattern3 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter the Number : ");
		int n = sc.nextInt();
		int row=0, col, num;
		
		while(row < n)
		{
			col = 0;
			num = 1;
			while(col <= row)
			{
				if((col == 0))
				{
					System.out.print(num + " ");
				}
				
				else
				{
					num = (num*(row-col))/(col+1);
					System.out.print(num + " ");
				}
				col++;
			}
			
			System.out.println();
			row++;
		}

	}

}
